{

  "description": "Skip automatically Youtube ads",
  "manifest_version": 2,
  "name": "Ad Skipper",
  "version": "1.0",
  "homepage_url": "https://github.com/StephaneBranly/",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["skipper.js"]
    }
  ]

}
